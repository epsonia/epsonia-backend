// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Team {
  id       Int @id @default(autoincrement())
  score    Int
  // winScore Int -- Not really a thing in Epsonia YET
  linScore Int
}
